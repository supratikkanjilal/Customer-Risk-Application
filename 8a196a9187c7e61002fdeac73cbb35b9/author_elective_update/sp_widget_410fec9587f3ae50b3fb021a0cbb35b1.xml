<?xml version="1.0" encoding="UTF-8"?><record_update table="sp_widget">
    <sp_widget action="DELETE">
        <category>custom</category>
        <client_script><![CDATA[function($scope, $location, spUtil) {
  // --- STATE ---
  $scope.page     = parseInt($location.search().page, 10) || 1;
  $scope.pageSize = parseInt($location.search().pageSize, 10) || 10; // fixed size
  $scope.sortDir  = ($location.search().dir === 'desc') ? 'desc' : 'asc';
  $scope.total    = 0;

  // --- EXISTING ---
  $scope.selectCustomer = function(customer) {
    $scope.selectedCustomer = customer;
    $location.search('table', 'CustomerProfile');
    $location.search('number', customer.number);
    syncUrl();
  };
  $scope.getRiskStyle = function(risk) {
    switch ((risk || '').toLowerCase()) {
      case 'high': return { color: 'red', 'font-weight': 'bold' };
      case 'medium': return { color: 'orange', 'font-weight': 'bold' };
      case 'low': return { color: 'green', 'font-weight': 'bold' };
      default: return {};
    }
  };

  // --- HELPERS ---
  function totalPages() {
    return Math.max(1, Math.ceil($scope.total / $scope.pageSize));
  }
  function syncUrl() {
    $location.search('page', $scope.page);
    $location.search('pageSize', $scope.pageSize);
    $location.search('dir', $scope.sortDir);
  }
  function load() {
    spUtil.update($scope, {
      page: $scope.page,
      pageSize: $scope.pageSize,
      sortDir: $scope.sortDir
    }).then(function(resp) {
      $scope.total = (resp && resp.data && resp.data.total) || 0;
      var tp = totalPages();
      if ($scope.page > tp) $scope.page = tp;
      syncUrl();
    });
  }

  // --- SORT ---
  $scope.setSortByNumber = function() {
    $scope.sortDir = ($scope.sortDir === 'desc') ? 'asc' : 'desc';
    $scope.page = 1;
    load();
  };
  $scope.getSortIcon = function() {
    return $scope.sortDir === 'desc' ? '▼' : '▲';
  };

  // --- FIXED: PREVIOUS / NEXT with event protection ---
  $scope.prevPage = function($event) {
    if ($event) { $event.preventDefault(); $event.stopPropagation(); }
    if ($scope.page > 1) {
      $scope.page--;
      load();
    }
  };

  $scope.nextPage = function($event) {
    if ($event) { $event.preventDefault(); $event.stopPropagation(); }
    var tp = totalPages();
    if ($scope.page < tp) {
      $scope.page++;
      load();
    }
  };

  // Initial load
  load();
}]]></client_script>
