        <script><![CDATA[(function() {
  // ---- CONFIG ----
  var table = 'x_cpszo_spoke_customer_profile'; // your table
  var nameField   = 'name';
  var numberField = 'number';
  var riskField   = 'risk_band';

  var defaultPageSize = 10;
  var maxPageSize = 100;

  // ---- INPUTS FROM CLIENT ----
  input = input || {};
  var page     = parseInt(input.page, 10) || 1;
  var pageSize = Math.min(Math.max(parseInt(input.pageSize, 10) || defaultPageSize, 1), maxPageSize);
  var sortDir  = (input.sortDir === 'desc') ? 'desc' : 'asc'; // sort by Number only

  // ---- OUTPUT SKELETON ----
  data.meta = {
    page: page,
    pageSize: pageSize,
    sortDir: sortDir,
    table: table,
    fields: { name: nameField, number: numberField, risk: riskField }
  };
  data.customers = [];
  data.total = 0;
  data.error = null;

  // ---- VALIDATE TABLE ----
  try {
    var grCheck = new GlideRecord(table);
    if (!grCheck.isValid()) {
      data.error = 'Invalid table: ' + table;
      return;
    }
  } catch (e) {
    data.error = 'Unable to access table: ' + table + ' (' + e.message + ')';
    return;
  }

  // ---- COUNT (all records) ----
  try {
    var ga = new GlideAggregate(table);
    ga.addAggregate('COUNT');
    ga.query();
    if (ga.next()) {
      data.total = parseInt(ga.getAggregate('COUNT'), 10) || 0;
    }
  } catch (errCount) {
    data.error = 'Count error: ' + errCount.message;
    return;
  }

  if (data.total === 0) {
    data.customers = [];
    return;
  }

  // ---- PAGE WINDOW ----
  var offset = (page - 1) * pageSize;

  // ---- DATA QUERY (ACL-aware) ----
  try {
    var gr = new GlideRecordSecure(table);

    // Server-side sort by Number (lexicographic)
    if (sortDir === 'desc') gr.orderByDesc(numberField); else gr.orderBy(numberField);

    gr.query();

    // Manually skip offset rows (compatible across releases)
    var i = 0, returned = 0;
    while (gr.next()) {
      if (i++ < offset) continue;

      data.customers.push({
        sys_id: gr.getUniqueValue(),
        name: gr.getDisplayValue(nameField),
        number: gr.getDisplayValue(numberField),
        risk_bond: gr.getDisplayValue(riskField)
      });

      if (++returned >= pageSize) break;
    }
  } catch (errData) {
    data.error = 'Data fetch error: ' + errData.message;
  }
})();]]></script>
