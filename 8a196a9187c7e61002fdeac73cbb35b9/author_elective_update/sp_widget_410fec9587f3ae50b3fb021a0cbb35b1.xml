        <template><![CDATA[<!-- Optional title -->
<h4 style="margin: 6px 0;">Customers</h4>

<table class="table table-bordered table-hover">
  <thead>
    <tr>
      <th>Name</th>
      <th style="cursor:pointer;" ng-click="setSortByNumber()">
        Number
        <span style="font-size: 11px; margin-left: 4px;">{{ getSortIcon() }}</span>
      </th>
      <th>Risk Bond</th>
    </tr>
  </thead>
  <tbody>
    <tr ng-repeat="customer in data.customers" ng-click="selectCustomer(customer)">
      <td>{{customer.name}}</td>
      <td>{{customer.number}}</td>
      <td ng-style="getRiskStyle(customer.risk_bond)">{{customer.risk_bond}}</td>
    </tr>
    <tr ng-if="(!data.customers || data.customers.length === 0)">
      <td colspan="3" class="text-center text-muted">No records found</td>
    </tr>
  </tbody>
</table>

<!-- Inline icon-style pager: ‹ Previous | Next › -->
<div class="row" style="margin-top: 8px;">
  <div class="col-sm-12 text-right" ng-if="total > 0">
    <!-- Previous (disabled on first page) -->
    <span ng-if="page <= 1" class="text-muted pager-link-disabled">‹ Previous</span>
    <a ng-if="page > 1"
       class="pager-link"
       role="button"
       tabindex="0"
       ng-click="prevPage($event)"
       ng-keypress="$event.keyCode===13 || $event.keyCode===32 ? prevPage($event) : null"
       aria-label="Previous page"
       title="Previous page">‹ Previous</a>

    <span class="pager-sep">|</span>

    <!-- Next (disabled on last page) -->
    <a ng-if="page < Math.max(1, Math.ceil(total / pageSize))"
       class="pager-link"
       role="button"
       tabindex="0"
       ng-click="nextPage($event)"
       ng-keypress="$event.keyCode===13 || $event.keyCode===32 ? nextPage($event) : null"
       aria-label="Next page"
       title="Next page">Next ›</a>
    <span ng-if="page >= Math.max(1, Math.ceil(total / pageSize))"
          class="text-muted pager-link-disabled">Next ›</span>
  </div>
</div>]]></template>
